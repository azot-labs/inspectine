{
  "extends": "./.wxt/tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "@": ["./src/extension"],
      "@/*": ["./src/extension/*"],
      "~": ["./src/extension"],
      "~/*": ["./src/extension/*"],
      "@@": ["."],
      "@@/*": ["./*"],
      "~~": ["."],
      "~~/*": ["./*"],
      "@orlan/lib": ["./src/lib"],
      "@orlan/lib/*": ["./src/lib/*"]
    }
  }
}
